@using UsersAndAwards.Dependences;
@using UsersAndAwards.BLL.Interfaces;

@{
    IRegistratorLogic regAdmin = DependencyResolver.RegistratorLogic;
    string userID = Request["userID"];

    if (string.IsNullOrWhiteSpace(userID))
    {
        Response.Redirect("~/Pages/EditRole.cshtml");
    }

    else
    {
        regAdmin.AddRoleAdmin(Guid.Parse(userID));
        Response.Redirect("~/Pages/EditRole.cshtml");
    }
}